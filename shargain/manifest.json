{
  "name": "SharGain",
  "description": "Chrome extension for SharGain",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "/images/icon.png"
    }
  },
  "icons": {
    "128": "/images/icon.png"
  },
  "options_page": "options.html"
}
